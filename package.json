{
  "name": "imdb-search",
  "version": "1.6.0",
  "description": "A simple extension that displays torrents for IMDB movies and series directly onto the page",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --debug --watch --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --progress --colors",
    "prettier": "./node_modules/.bin/prettier --tab-width 4 --print-width 120 --write \"+(src)/**/**.+(js|ts|scss|css|html)\"",
    "release": "npm run build && node build.js"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/extension/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^1.3.0",
    "axios": "^0.16.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-2017": "^0.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "loglevel": "^1.4.1",
    "materialize-css": "^0.98.1",
    "moxios": "^0.4.0",
    "node-sass": "^4.13.1",
    "prettier": "^2.0.4",
    "sass-loader": "^6.0.3",
    "script-loader": "^0.7.0",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3"
  }
}
