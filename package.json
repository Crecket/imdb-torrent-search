{
  "name": "imdb-search",
  "version": "1.1.0",
  "description": "A simple extension that displays torrents for IMDB movies and series directly onto the page",
  "main": "app.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --debug --watch --progress --colors",
    "build": "cross-env NODE_ENV=production webpack --progress --colors",
    "release": "node build.js",
    "test": "jest --coverage",
    "test:dev": "jest --watch --coverage"
  },
  "engines": {
    "node": ">=7.8.0"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/extension/"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "archiver": "^1.3.0",
    "axios": "^0.16.1",
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-2017": "^0.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
    "enzyme": "^2.8.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "flexboxgrid": "^6.3.1",
    "glob": "^7.1.1",
    "gsap": "^1.19.1",
    "html-webpack-plugin": "^2.26.0",
    "jest": "*",
    "loglevel": "^1.4.1",
    "materialize-css": "^0.98.1",
    "moxios": "^0.4.0",
    "node-sass": "^4.5.2",
    "sass-loader": "^6.0.3",
    "spinkit": "^1.2.5",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3"
  }
}
